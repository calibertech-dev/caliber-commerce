<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
  <fullName>Percent_Range</fullName>
  <active>true</active>
  <description>Percent must be between 0 and 100.</description>
  <errorConditionFormula>
NOT( OR( ISBLANK(Percent__c), AND(Percent__c &gt;= 0, Percent__c &lt;= 100) ) )
  </errorConditionFormula>
  <errorDisplayField>Percent__c</errorDisplayField>
  <errorMessage>Percent must be between 0 and 100.</errorMessage>
</ValidationRule>
